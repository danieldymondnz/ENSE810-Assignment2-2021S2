      state 128130 "GraphDisplay"
	  uml_do_activity "Execute _displayGraph()
Check flags and speed"
	comment "Displays the graphs of both Temperature and Humidity on the HAT Display."
	transition 128002 "<transition>"
	  on state_ref 128258 // TemperatureAlertDisplay
	    uml_trigger "_tempWarning == 1"
	  external 
	end

	transition 128642 "<transition>"
	  on state_ref 128386 // HumidityAlertDisplay
	    uml_trigger "_RHWarning == 1"
	  external 
	end

	transition 128898 "<transition>"
	  on state_ref 128514 // SpeedAlertDisplay
	    uml_trigger "_speedWarning == 1"
	  external 
	end

	transition 129026 "<transition>"
	  on state_ref 128642 // AccelerationAlertDisplay
	    uml_trigger "_accelerationWarning == 1"
	  external 
	end

	transition 129282 "<transition>"
	  on pseudostate_ref 128258 // exit_point Exit
	    uml_trigger "_terminate()
method
called"
	  external 
	end

	transition 129410 "<transition>"
	  on state_ref 128130 // GraphDisplay
	    uml_trigger "while
loop"
	  external 
	end
      end

      state 128258 "TemperatureAlertDisplay"
